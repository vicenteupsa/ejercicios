#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> numeros = {5, 2, 8, 1, 9, 3};
    double suma = 0;
    
    for(int i = 0; i < numeros.size(); i++) {
        suma += numeros[i];
    }
    
    double promedio = suma / numeros.size();
    cout << "El promedio es: " << promedio << endl;
    return 0;
}
