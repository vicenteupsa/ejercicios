#include <iostream>
#include <vector>
using namespace std;

string numeroATexto(int numero) {
    vector<string> unidades = {"CERO", "UNO", "DOS", "TRES", "CUATRO", "CINCO", 
                              "SEIS", "SIETE", "OCHO", "NUEVE", "DIEZ"};
    vector<string> especiales = {"ONCE", "DOCE", "TRECE", "CATORCE", "QUINCE", 
                                "DIECISEIS", "DIECISIETE", "DIECIOCHO", "DIECINUEVE"};
    
    if(numero >= 0 && numero <= 10) {
        return unidades[numero];
    } else if(numero >= 11 && numero <= 19) {
        return especiales[numero - 11];
    } else if(numero == 20) {
        return "VEINTE";
    } else {
        return "NUMERO FUERA DE RANGO";
    }
}

int main() {
    int numero;
    cout << "Ingrese un nÃºmero (0-20): ";
    cin >> numero;
    
    cout << numeroATexto(numero) << endl;
    return 0;
}
