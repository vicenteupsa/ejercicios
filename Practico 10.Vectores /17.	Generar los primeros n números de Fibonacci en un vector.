#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de números Fibonacci a generar: ";
    cin >> n;
    
    vector<long long> fibonacci;
    
    if(n >= 1) fibonacci.push_back(0);
    if(n >= 2) fibonacci.push_back(1);
    
    for(int i = 2; i < n; i++) {
        long long siguiente = fibonacci[i-1] + fibonacci[i-2];
        fibonacci.push_back(siguiente);
    }
    
    cout << "Los primeros " << n << " números de Fibonacci:" << endl;
    for(int i = 0; i < fibonacci.size(); i++) {
        cout << fibonacci[i] << " ";
    }
    cout << endl;
    
    return 0;
}
