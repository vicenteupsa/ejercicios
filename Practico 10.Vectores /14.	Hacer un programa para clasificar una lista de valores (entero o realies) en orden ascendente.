#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> numeros = {5, 2, 8, 1, 9, 3};
    
    cout << "Vector original: ";
    for(int num : numeros) {
        cout << num << " ";
    }
    cout << endl;
    
    // Ordenar usando algoritmo de burbuja
    for(int i = 0; i < numeros.size() - 1; i++) {
        for(int j = 0; j < numeros.size() - i - 1; j++) {
            if(numeros[j] > numeros[j+1]) {
                // Intercambiar
                int temp = numeros[j];
                numeros[j] = numeros[j+1];
                numeros[j+1] = temp;
            }
        }
    }
    
    cout << "Vector ordenado: ";
    for(int num : numeros) {
        cout << num << " ";
    }
    cout << endl;
    
    return 0;
}
