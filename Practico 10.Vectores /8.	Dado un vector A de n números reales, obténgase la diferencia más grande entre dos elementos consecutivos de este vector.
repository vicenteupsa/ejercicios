#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

int main() {
    vector<double> numeros = {1.5, 3.2, 2.1, 5.8, 4.3, 1.2};
    double diferenciaMaxima = 0;
    
    for(int i = 0; i < numeros.size() - 1; i++) {
        double diferencia = abs(numeros[i] - numeros[i+1]);
        if(diferencia > diferenciaMaxima) {
            diferenciaMaxima = diferencia;
        }
    }
    
    cout << "La diferencia mÃ¡xima entre elementos consecutivos es: " 
         << diferenciaMaxima << endl;
    return 0;
}
