#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> numeros;
    vector<int> sinDuplicados;
    int cantidad, numero;
    
    // Pedir al usuario que ingrese los datos
    cout << "¿Cuántos números deseas ingresar? ";
    cin >> cantidad;
    
    cout << "Ingresa " << cantidad << " números:" << endl;
    for(int i = 0; i < cantidad; i++) {
        cout << "Número " << i + 1 << ": ";
        cin >> numero;
        numeros.push_back(numero);
    }
    
    // Mostrar vector original
    cout << "\nVector original: ";
    for(int num : numeros) {
        cout << num << " ";
    }
    cout << endl;
    
    // Ordenar para facilitar la eliminación de duplicados
    sort(numeros.begin(), numeros.end());
    
    // Agregar primer elemento
    if(!numeros.empty()) {
        sinDuplicados.push_back(numeros[0]);
    }
    
    // Agregar elementos no duplicados
    for(int i = 1; i < numeros.size(); i++) {
        if(numeros[i] != numeros[i-1]) {
            sinDuplicados.push_back(numeros[i]);
        }
    }
    
    cout << "Vector sin duplicados: ";
    for(int num : sinDuplicados) {
        cout << num << " ";
    }
    cout << endl;
    
    return 0;
}
