#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<string> nombres;
    vector<double> notas;
    
    // Agregar algunos alumnos de ejemplo
    nombres.push_back("Ana");
    notas.push_back(85.5);
    
    nombres.push_back("Luis");
    notas.push_back(92.0);
    
    // Mostrar lista
    cout << "Lista de alumnos:" << endl;
    for(int i = 0; i < nombres.size(); i++) {
        cout << nombres[i] << " - " << notas[i] << endl;
    }
    
    return 0;
}
