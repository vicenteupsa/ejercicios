#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> A = {1, 2, 3, 4};
    vector<int> B = {5, 6, 7, 8, 9};
    vector<int> concatenado;
    
    // Agregar elementos del vector A
    for(int num : A) {
        concatenado.push_back(num);
    }
    
    // Agregar elementos del vector B
    for(int num : B) {
        concatenado.push_back(num);
    }
    
    cout << "Vector concatenado: ";
    for(int num : concatenado) {
        cout << num << " ";
    }
    cout << endl;
    
    return 0;
}
